{
  "manifest_version": 3,
  "name": "Chrome-Kids",
  "description": "Um ambiente online e seguro para uma crian√ßa, fazendo analise-ses de todos os sites e os seus conteudos",
  "version": "1.2",
  "background": {
    "service_worker": "background.js"
  },
  "permissions": [
    "webNavigation",
    "scripting",
    "storage"
  ],
  "host_permissions": ["<all_urls>"],
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
}
